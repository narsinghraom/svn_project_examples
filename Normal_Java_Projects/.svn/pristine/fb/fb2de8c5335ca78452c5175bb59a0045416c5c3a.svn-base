package narsingh.collections;

import java.util.ArrayList;
import java.util.*;
import java.util.Iterator;

public class ListExample {
	
	public void arrayList()
	{
		ArrayList al=new ArrayList();
		al.add("1");
		al.add("Narsingh");
		al.add(10.2);
		al.add(1);
		al.add('s');
		al.add(false);
	
		System.out.println(al);
		Iterator i1=al.iterator();
		while(i1.hasNext())
		{
			
			
			System.out.println(i1.next());
		}
		
	}

	public void arrayListString()
	{
		ArrayList<String> al=new ArrayList<String>();
		al.add("1");
		al.add("narsingh");
		al.add("shekar");
		al.add("1");
		Iterator i1=al.iterator();
		while(i1.hasNext())
		{
			System.out.println(i1.next());
		}
		ArrayList<Integer> ali=new ArrayList<Integer>();
		ali.add(1);
		ali.add(2);
		ali.add(3);
		Iterator<Integer> i11=ali.iterator();
		while(i11.hasNext())
		{
			System.out.println(i11.next());
		}
		
	}
	public void shekar()
	{
		ArrayList<Double> al=new ArrayList<Double>();
		al.add(10.222);
		al.add(19800.00);
		ArrayList<Boolean> alb=new ArrayList<Boolean>();
		alb.add(false);
		alb.add(true);
		
		Vector v=new Vector();
		v.add(1);
		v.add("narsingh");
		Vector<String> v1=new Vector<String>();
		v1.add("narsingh");
		v1.add("shekar");
		v1.add("lantar pagdi");
		System.out.println(v);
		System.out.println(v1);
		
		
		
	}
	public static void main(String[] args) 
	{
		ListExample le=new ListExample();
		//le.arrayList();
		//le.arrayListString();

		le.shekar();
	}

}
